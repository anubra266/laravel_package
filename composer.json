{
    "name": "anubra266/laravel_package",
    "description": "",
    "keywords": [
        "anubra266",
        "laravel_package"
    ],
    "homepage": "https://github.com/anubra266/laravel_package",
    "license": "MIT",
    "authors": [
        {
            "name": "Abraham",
            "email": "anubra266@gmail.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Anubra266\\LaravelPackage\\": "src",
            "Anubra266\\LaravelPackage\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anubra266\\LaravelPackage\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anubra266\\LaravelPackage\\LaravelPackageServiceProvider"
            ],
            "aliases": {
                "LaravelPackage": "Anubra266\\LaravelPackage\\LaravelPackageFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/spatie"
        },
        {
            "type": "other",
            "url": "https://spatie.be/open-source/support-us"
        }
    ]
}
